#include <stdio.h>
int main() {
    int n[3], temp;
    for(int i=0; i<3; i++)
        scanf("%d", &n[i]);
    
    for(int i=0; i<2; i++) {
        for(int j=i+1; j<3; j++) {
            if(n[i] < n[j]) {
                temp = n[j];
                n[j] = n[i];
                n[i] = temp;
            }
        }
    }
    printf("%d", n[1]);   
    return 0;
}
